/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.long-text {
  display: inline-block !important;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.no-data, .loading {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 14px;
  color: #999;
}
.console {
  align-items: center;
  margin-bottom: 12px;
  padding-right: 12px;
  height: 36px;
  border-radius: 4px;
  border-left: 4px solid #666;
  background-color: #f9f9f9;
  color: #999;
  font-size: 14px;
  display: flex;
  flex-flow: nowrap row;
  justify-content: space-between
}

.console .key {
  padding-left: 10px;
  color: #999;
  letter-spacing: 2px;
}

.console .value {
  color: #333;
}
.bottom-tip {
  position: absolute;
  bottom:-30px;
  width:100%;
  font-size:12px;
  color:#999;
  text-align:center;
  padding-bottom: 12px;
}
