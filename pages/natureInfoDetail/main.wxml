    <detail-container attributes="{{attributes}}" detail="{{natureInfo}}">
      <view>
        <composition title="来源" wx:if="{{hasOrigin}}" size="{{size}}" composition="{{natureInfo.origin}}"></composition>
        <composition title="可制作科技" wx:if="{{hasCreate}}" size="{{size}}" composition="{{natureInfo.createScience}}"></composition>
