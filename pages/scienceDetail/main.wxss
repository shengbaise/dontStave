page {
  height: 100%;
}
.science-technology-detail {
  position: relative;
  height: 100%
}

.science-technology-detail .view {
  position: absolute;
  top: 0;
  height: 100%;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
}
.attrs {
  font-size: 15px
}

.attrs .aggressivity,
.attrs .durable {
  width: 50px;
  display: inline-block;
  margin-right: 12px;
  font-size: 12px;
  color: #999;
  letter-spacing: 2px;
}
.attr-contanier {
  display: flex;
  align-items: center;
}
.attr-value {
  font-size: 14px;
  color: #606266;
  text-align: left;
  font-family: Arial Rounded MT Bold;
  letter-spacing: 2px;
}
