page {
  width:100%;
  height:100%;
}
.search {
  display: flex;
  flex-flow: column;
  height: 100%;
}
.tip {
  color: #999;
  font-size: 12px;
  text-align: left;
  padding: 8px;
  background-color: #f5f5f5;
}
.view {
  height: 100%;
  flex-grow: 1;
  overflow: scroll;
  -webkit-overflow-scrolling: touch;
}
