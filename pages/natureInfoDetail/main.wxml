<view class="nature-intro-detail">
  <scroll-view scroll-y="{{true}}" class="view">
    <common-good wx:if="{{natureInfo}}" good="{{natureInfo}}" hasFeedback="{{true}}">
      <view class="attrs">
        <view class="attr-contanier" wx:if="{{natureInfo.damage}}">
          <view class="aggressivity">攻击力</view>
          <view class="attr-value">{{natureInfo.damage}}</view>
        </view>
        <view class="attr-contanier" wx:if="{{natureInfo.durable}}">
          <view class="durable">耐久</view>
          <view class="attr-value">{{natureInfo.durable}}</view>
        </view>
      </view>
    </common-good>
